body
  padding: 0px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif

input
  font-size:18px;
  padding:10px 10px 10px 5px;
  display:block;
  height: auto;
  width:300px;
  border:none;
  border-bottom:1px solid #757575;

a
  color: inherit;
  text-decoration: none;

.material-icons
  float: right;
  margin-left: 0px;
  height: 25px;
  width: 25px;
  line-height: 48px;
  margin-top: 4px;
  //margin: 0 32px 0 0;
  color: rgba(0, 0, 0, 0.54);

.mini
  text-decoration: none;
  border-radius: 10px;
  border-color: #607d8b;
  letter-spacing: .5px;
  height: 50px;
  line-height: 50px;
  padding: 0 2rem;
  vertical-align: middle;
  text-transform: uppercase;
  text-align: center;

.mini:hover
  background-color: #90a4ae;
  color: #fff;

.btn, .btn-large
  text-decoration: none;
  background-color: #eceff1;
  text-align: center;
  letter-spacing: .5px;
  transition: .2s ease-out;
  cursor: pointer;
  border: none;
  border-radius: 2px;
  height: 40px;
  line-height: 36px;
  padding: 0 2rem;
  text-transform: uppercase;
  vertical-align: middle;
  -webkit-tap-highlight-color: transparent;

.btn:hover
  color: #fff;
  background-color: #90a4ae

.row
  margin-left: auto;
  margin-right: auto;
  margin-bottom: 20px;

.row .col
  float: left;
  box-sizing: border-box;
  padding: 0 0.75rem;
  min-height: 1px;

